{
  "name": "fuzzy-neon",
  "version": "0.1.5",
  "description": "Collection of fuzzy string matching algorithms written in rust",
  "main": "index.node",
  "scripts": {
    "build": "cargo-cp-artifact -nc index.node -- cargo build --message-format=json-render-diagnostics",
    "build-debug": "npm run build --",
    "build-release": "npm run build -- --release",
    "install": "npm run build-release",
    "test": "cargo test"
  },
  "author": "rupert648",
  "license": "ISC",
  "devDependencies": {
    "cargo-cp-artifact": "^0.1"
  },
  "keywords": [
    "string-matching",
    "rust",
    "neon",
    "fuzzy",
    "approximate"
  ]
}
